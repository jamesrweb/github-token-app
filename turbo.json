{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["public/**", "elm-stuff/**"]
    },
    "dev": {
      "cache": false,
      "outputs": ["public/**", "elm-stuff/**"]
    },
    "format": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "start": {
      "outputs": []
    }
  }
}
